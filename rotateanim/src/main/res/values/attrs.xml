<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundNumProgressView">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="percentTextColor" format="color" />
        <attr name="percentTextSize" format="dimension" />
        <attr name="maxProgress" format="integer"></attr>
        <attr name="percentTextDisplayable" format="boolean"></attr>
        <attr name="rotateOrientation">
            <enum name="counterclockwise" value="0"></enum>
            <enum name="clockwise" value="1"></enum>
        </attr>
        <attr name="style">
            <enum name="stroke" value="0"></enum>
            <enum name="fill" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>
