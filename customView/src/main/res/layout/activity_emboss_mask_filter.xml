<?xml version="1.0" encoding="utf-8"?>
<LinearLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_emboss_mask_filter"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.customview.activity.colorFilter.EmbossMaskFilterActivity">

    <TextView
        android:id="@+id/tv_light_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:text="woshi"/>

    <com.example.customview.customview.MaskFilterViewPackage.EmbossMaskFilterView
        android:id="@+id/emf_embossMaskFilterView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>
